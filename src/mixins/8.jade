.nav-collapse(collapse="isCollapse")
  ul.nav.pull-right
    li
      a(ng-href="{{ user.profileUrl }}", target="_blank")
        img.img-rounded(ng-src="https://graph.facebook.com/{{ user.id }}/picture?type=normal")
        | {{ user.displayName }}
    li
      a(ng-if="user.$name == '100001053090034'", href="", ng-click="resetFB()")
        | Reset FB
    li
      a(ng-click="auth.login('facebook', {rememberMe: true, scope: 'email'})")
        | Facebook Login 